{
	"name": "server-ufsj-curriculum",
	"version": "1.0.0",
	"scripts": {
		"start": "nodemon",
		"build": "npx tsc",
		"prepare": "npx ts-patch install -s"
	},
	"dependencies": {
		"cors": "^2.8.5",
		"crypto": "^1.0.1",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"mongoose": "^7.0.3"
	},
	"devDependencies": {
		"@types/cors": "^2.8.13",
		"@types/express": "^4.17.17",
		"@types/node": "^18.15.11",
		"nodemon": "^2.0.22",
		"ts-node": "^10.9.1",
		"ts-patch": "^2.1.0",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.0.3"
	},
	"nodemonConfig": {
		"ignore": [
			"**/*.test.ts",
			"**/*.spec.ts",
			".git",
			"node_modules"
		],
		"watch": [
			"src"
		],
		"exec": "node -r tsconfig-paths/register -r ts-node/register ./src/index.ts",
		"ext": "ts, js"
	}
}
